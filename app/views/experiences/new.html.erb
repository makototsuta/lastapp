<%= form_with(model: @experience, local: true) do |form| %>
  <div class="experience_title">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div class="experience_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>

